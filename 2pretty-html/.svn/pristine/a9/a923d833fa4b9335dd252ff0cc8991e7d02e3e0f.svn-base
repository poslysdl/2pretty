.head-user {    -moz-box-sizing: border-box; position:relative;}
.head-user .tilesp {overflow: hidden;
    position: relative;}
	
.head-user .head-info { display:block;}

.head-user .avatar {
    
    position: absolute;
    z-index: 2;
	bottom: -110px;
    display: block;
    border: 5px solid #FFFFFF !important;
    	-webkit-border-radius: 50% !important;
	-moz-border-radius: 50% !important;
	border-radius: 50% !important;
    left: 40px;
    
}


.head-user .bguser { background: rgba(167, 207, 95, .8); padding-left: 240px;}

.head-user .avatar img { 
    width: 200px;height: 200px;
	-webkit-border-radius: 48% !important;
	-moz-border-radius: 48% !important;
	border-radius: 48% !important;
    position: static;}
	
.head-user .user-text {
	
	
    -moz-box-sizing: border-box;
   
    bottom: 10px;
    left: 0;
	margin-left: 34px; 
    position: absolute;
    width: 720px;
}



.ie .head-user .user-text { margin-left:34px;}

.head-user .user-text h1 {
   
	 overflow: hidden;
    text-overflow: ellipsis;
	margin:0;
	text-align:left;
    white-space: nowrap;
}

.head-user .user-text h1 a { color: #FFFFFF;
    font-size: 28px;}


.head-user .user-text p 
{
	 color: #FFFFFF;
	 text-align:left;
	 margin-top:2px;
	 padding-bottom:8px;
	text-transform:uppercase;
	overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
	font-size: 13px;}	
	
	
.head-user .head-but{
    
    position: absolute;
    right: 40px;
	bottom: 20px;
    z-index: 3;
}	




.head-user .head-but .buton-user 

{display: block; margin:0; padding:0;}

.head-user .head-but .buton-user .follow {}
