<script>
$(document).ready(function(){
 $(this).scrollTop(0);
 $(window).scroll(function(){
  var y = $(window).scrollTop();
  if( y > 0 ){
      
	   $("#top-shadow").addClass("topsha");
	   $("#right-slide").addClass("rightsha");
  } else {
      
	  $("#top-shadow").removeClass("topsha");
	  $("#right-slide").removeClass("rightsha");
  }
 });
})
</script> 
